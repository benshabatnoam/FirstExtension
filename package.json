{
	"name": "google-translate",
	"displayName": "Google Translate",
	"description": "Use this extension to translate your code to Hebrew.",
	"version": "1.0.0",
	"publisher": "benshabatnoam",
	"repository": {
		"type": "git",
		"url": "https://github.com/benshabatnoam/FirstExtension.git"
	},
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.translate"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.translate",
				"title": "Translate"
			}
		],
		"keybindings": [
			{
				"command": "extension.translate",
				"key": "shift+ctrl+t"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"tslint": "^5.8.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"google-translate": "^2.0.5"
	}
}
